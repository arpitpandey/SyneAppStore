<%= javascript_include_tag "jquery.lightbox-0.5" %><%= javascript_include_tag "jquery" %><%= stylesheet_link_tag "jquery.lightbox-0.5" %> <script type="text/javascript"> $(document).ready(function(){		 $('a.prod_details').click(function(){				 $('.details').css({"width":"585px","float":"left","padding":"5px 10px","visibility":"visible",});				 $('.center_content').replaceWith('.details');		 });		 		 		 //code for accordian list		 //slides the element with class "menu_...body" when paragraph with class "menu_head" is clicked 	$("#firstpane li.odd").click(function()    {		$(this).css({backgroundImage:"url(down.png)"}).next("div.menu_body").slideToggle(300).siblings("div.menu_body").slideUp("slow");       	$(this).siblings().css({backgroundImage:"url(left.png)"});	});	//slides the element with class "menu_body" when mouse is over the paragraph	$("#secondpane li.odd").mouseover(function()    {	     $(this).css({backgroundImage:"url(down.png)"}).next("div.menu_body").slideDown(500).siblings("div.menu_body").slideUp("slow");         $(this).siblings().css({backgroundImage:"url(left.png)"});	}); }); </script> <style type="text/css">body {	margin: 10px auto;	font: 75%/120% Verdana,Arial, Helvetica, sans-serif;}.menu_list {		width: 150px;}.odd {	padding: 5px 10px;	cursor: pointer;	position: relative;	margin:1px;    font-weight:bold;    background: #eef4d3 url(left.png) center right no-repeat;}.menu_body {	display:none;}.menu_body a{  display:block;  color:#006699;  background-color:#EFEFEF;  padding-left:10px;  font-weight:bold;  text-decoration:none;}.menu_body a:hover{  color: #000000;  text-decoration:underline;  }</style>  <div class="left_content">      <div class="title_box">Categories</div>		  <div id="firstpane" class="menu_list"> <!--Code for menu starts here-->								<ul class="left_menu">					<li class="odd" id="cat1">Applications</li>					<div class="menu_body">							 <%= link_to "Phone App",phoneapp_path , {:class => "catbtn" } %>							 <%= link_to "Windows App",winapp_path , {:class => "catbtn" } %>							 <%= link_to "Tools",toolpp_path, {:class => "catbtn" } %>					 </div>					<li class="odd" id="cat1"><%= link_to "Accesories",accesoriapp_path %></li>				</ul>										  </div> 	   	              <!--<div class="banner_adds"> <a href="http://all-free-download.com/free-website-templates/"><img src="images/bann2.jpg" alt="" border="0" /></a> </div>-->	  </div>	   <div class="center_content" id="div1">	 <%for prod in @product%>		<% if prod.isactive? %>					  <div class="prod_box">			<div class="top_prod_box"></div>			<div class="center_prod_box">			  <div class="product_title"><a href="details.html"><%= prod.name %></a></div>			  <div class="product_img"><a href="<%=prod.image.url%>" title="SyneAppStore" id="a1" rel="lightbox"><%=image_tag prod.image.url,:width => 95, :height => 95 %></a></div>			  <div class="prod_price"> <span class="price"><%= prod.price %>$</span></div>			</div>			<div class="bottom_prod_box"></div>			 <div class="prod_details_tab"> <a href="#" title="header=[Add to cart] body=[&nbsp;] fade=[on]">			<%=link_to image_tag("../images/cart.gif",:class => "left_bt", :id => "addcart1"),addcart_path(:id =>prod.id)%></a> <a href="#"><img src="../images/favs.gif" alt="" border="0" class="left_bt" /></a> <a href="#" title="header=[Gifts] body=[&nbsp;] fade=[on]"><img src="../images/favorites.gif" alt="" border="0" class="left_bt" /></a><a class="prod_details" id="linkdetails">details</a></div>		  </div>		<%end%>	 <%end%> </div>	  <% if $sum>0 %><div class="right_content">        <div class="title_box">Cart Details</div>		<table bgcolor="#F5F5DC">			<tr>				<th class="list_th"><b>PName</b></th>				<th class="list_th"><b>QTY</b></th>				<th class="list_th"><b>Price</b></th>				<th class="list_th"><b>Delete</b></th>			</tr>			<%for cart_obj in @cartinfo%>				<tr>					<td><%= cart_obj.productName %></td>					<td><%= cart_obj.quantity %></td>					<td><%= cart_obj.productPrice %></td>					<td><%= link_to image_tag("/images/Icon_Delete.gif",:width => 15, :height => 15), cartdelete_path(:id =>cart_obj.id) %></td>				</tr>			<%end%>		</table>		<b>SubTotal : <%= $sum%></b><br/>		</br>		<center> <%=link_to image_tag("/images/checkout.png",:height=>26,:width=>70),makepayment_path%></center></div><%end%>	  